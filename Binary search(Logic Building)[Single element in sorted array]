class Solution {
public:
    int singleNonDuplicate(vector<int> &nums) {
        int r;
        for(int i=1;i<nums.size()-1;i++) {
    if(nums[i]!=nums[i-1]&&nums[i]!=nums[i+1])  r=nums[i];}
    if(nums.size()==1) r=nums[0];
    if(nums.size()>1) {
        if(nums[0]!=nums[1]) r=nums[0];
    if(nums[nums.size()-1]!=nums[nums.size()-2]) r=nums[nums.size()-1];
    }
        return r;
    }
};
