class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
         int n=nums.size();
        vector<int> a;
        int c=1;
        for(int i=0;i<n-1;i++){
            if(nums[i]!=nums[i+1]) {
                a.push_back(nums[i]);
                c++;
            }
        }
        a.push_back(nums[n-1]);
        for(int i=0;i<c;i++){
            nums[i]=a[i];
        }
        return c;
    }
};
