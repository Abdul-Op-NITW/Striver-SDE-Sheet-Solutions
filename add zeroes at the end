class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int k=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0){
                nums.erase(nums.begin()+i);
                k++;
                i--;
            }
        }
        for(int i=1;i<=k;i++){
            nums.push_back(0);
        }
    }
};
