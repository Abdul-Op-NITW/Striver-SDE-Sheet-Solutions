class Solution {
public:
    double myPow(double x, int n) {
        //your code goes here
        double res=1;
        double base =x;
        long long N=abs(n);
       while(N>0){
    if(N%2==1) {res*=base;}
      base=base*base;
      N/=2;
       }
      if(n<0) res=1/res;
      return res;
    }
};
